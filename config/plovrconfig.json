//The plovr condfig file for the surfforecaster.com project.
{
    "id": "whipple",
    "paths": [
    "../whipple.eig/"],
    // Do parameters.js and whipple.js need to be in inputs?
    // when i put them there, I get an uncaught ReferenceError: whipple is not
    // defined
    "inputs": ["../whipple.eig/main.js", "../whipple.eig/parameters.js", "../whipple.eig/whipple.js"],
    "level": "VERBOSE",
   //  "mode": "RAW",
//        "mode": "SIMPLE",
  "mode": "RAW",
    "externs":'../whipple.eig/externs.js',
    "define": {
    "goog.DEBUG": true
    },
    "checks": {
        // acceptable values are "ERROR", "WARNING", and "OFF" 
        "deprecated": "ERROR",
        "checkTypes": "ERROR",
        "nonStandardJsDocs": "ERROR",
        "undefinedVars": "ERROR"
    }
}
